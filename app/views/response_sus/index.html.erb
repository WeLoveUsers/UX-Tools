<p id="notice"><%= notice %></p>

<h1>Response Sus</h1>

<table>
  <thead>
    <tr>
      <th>Q1</th>
      <th>Q2</th>
      <th>Q3</th>
      <th>Q4</th>
      <th>Q5</th>
      <th>Q6</th>
      <th>Q7</th>
      <th>Q8</th>
      <th>Q9</th>
      <th>Q10</th>
      <th>Respondent</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_sus.each do |response_su| %>
      <tr>
        <td><%= response_su.Q1 %></td>
        <td><%= response_su.Q2 %></td>
        <td><%= response_su.Q3 %></td>
        <td><%= response_su.Q4 %></td>
        <td><%= response_su.Q5 %></td>
        <td><%= response_su.Q6 %></td>
        <td><%= response_su.Q7 %></td>
        <td><%= response_su.Q8 %></td>
        <td><%= response_su.Q9 %></td>
        <td><%= response_su.Q10 %></td>
        <td><%= response_su.respondent_id %></td>
        <td><%= response_su.project %></td>
        <td><%= link_to 'Show', response_su %></td>
        <td><%= link_to 'Edit', edit_response_su_path(response_su) %></td>
        <td><%= link_to 'Destroy', response_su, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

